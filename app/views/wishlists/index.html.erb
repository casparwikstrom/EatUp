
<div class="container text-center">
  <div class="row">
  <div class="wishlist">

  <h1><%= current_user.first_name %>s Wishlist</h1>
    <% @wishlists.each do |wishlist| %>
    <div>
      <%= wishlist.popup.title %>
      <%= link_to "Delete", wishlist_path(wishlist), method: :delete, data: { confirm: "Are you sure?" } %>
    </div>
    <!-- <% end %> -->
 </div>
  </div>
</div>
